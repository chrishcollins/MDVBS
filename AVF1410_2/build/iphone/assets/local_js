var ui=require("ui");Ti.Database.install("/database/weatherDB.sqlite","nameInfo");var tblData=[],read=function(){for(var e=Ti.Database.open("nameInfo"),i=e.execute("SELECT id, name, desc FROM asdTbl");i.isValidRow();)tblData.push({id:i.fieldByName("id"),name:i.fieldByName("name"),desc:i.fieldByName("desc")}),i.next();i.close(),e.close(),ui.peopleTbl(tblData)};exports.read=read;var create=function(e,i){var t=Ti.Database.open("nameInfo");t.execute("INSERT INTO asdTbl (name, desc) VALUES (?, ?)",e,i);t.lastInsertRowId;t.close(),tblData=[],ui.tabGroup.setActiveTab(ui.tab1)};exports.create=create;var update=function(){var e=Ti.Database.open("weatherDb");e.execute("UPDATE list SET city=?, currentTemp=? id id=?",ui.nameField.value,ui.descField.value),e.close(),tblData=[],read(),ui.tabGroup.setActiveTab(ui.tab1),ui.button.id=null,ui.button.edit=!1,ui.button.title="Add Person",ui.nameField.value="",ui.descField.value="",win2.title="Add Person",ui.tab2.title="Add"};exports.update=update;var del=function(e){var i=Ti.Database.open("nameInfo");i.execute("DELETE FROM list WHERE id=?",e),i.close(),tblData=[],read()};exports.del=del;