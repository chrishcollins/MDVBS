var Cloud=require("ti.cloud");Cloud.debug=!0,Cloud.Users.login({login:"user",password:"1234"},function(e){if(e.success){var i=e.users[0];Ti.API.info("Success:\nid: "+i.id+"\nsessionId: "+Cloud.sessionId+"\nfirst name: "+i.first_name+"\nlast name: "+i.last_name)}else Ti.API.info("Error:\n"+(e.error&&e.message||JSON.stringify(e)))}),exports.loginAppUser=function(e){Cloud.Objects.create({classname:"weather",fields:{data:e}},function(e){alert(e.success?"Success":"Error:\n"+(e.error&&e.message||JSON.stringify(e)))})},exports.Cloud=Cloud;