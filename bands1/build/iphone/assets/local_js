var ui=require("ui");Ti.Database.install("bandData.sqlite","nameInfo");var tblData=[],read=function(){for(var e=Ti.Database.open("nameInfo"),t=e.execute("SELECT id, name, desc FROM bandTbl");t.isValidRow();)tblData.push({id:t.fieldByName("id"),name:t.fieldByName("name"),desc:t.fieldByName("band")}),t.next();t.close(),e.close(),ui.peopleTbl(tblData)};exports.read=read;var create=function(e,t){var a=Ti.Database.open("nameInfo");a.execute("INSERT INTO bandTbl (name, desc) VALUES (?, ?)",e,t);a.lastInsertRowId;a.close(),tblData=[],ui.tabGroup.setActiveTab(ui.tab1)};exports.create=create;var update=function(){var e=Ti.Database.open("bandData");e.execute("UPDATE list SET city=?, band=? id id=?",ui.nameField.value,ui.descField.value),e.close(),tblData=[],read(),ui.tabGroup.setActiveTab(ui.tab1),ui.button.id=null,ui.button.edit=!1,ui.button.title="Add Person",ui.nameField.value="",ui.descField.value="",win2.title="Add Person",ui.tab2.title="Add"};exports.update=update;var del=function(e){var t=Ti.Database.open("nameInfo");t.execute("DELETE FROM list WHERE id=?",e),t.close(),tblData=[],read()};exports.del=del;