var Cloud=require("ti.cloud");Cloud.debug=!0,Cloud.Users.login({login:"user",password:"1234"},function(e){if(e.success){var t=e.users[0];Ti.API.info("Success:\nid: "+t.id+"\nsessionId: "+Cloud.sessionId+"\nfirst name: "+t.first_name+"\nlast name: "+t.last_name)}else Ti.API.info("Error:\n"+(e.error&&e.message||JSON.stringify(e)))}),exports.loginAppUser=function(e){Cloud.Objects.create({classname:"bands1",fields:{data:e}},function(e){alert(e.success?"Success":"Error:\n"+(e.error&&e.message||JSON.stringify(e)))})},exports.Cloud=cloud;