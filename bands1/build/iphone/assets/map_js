function createMap(){var t=Map.createAnnotation({latitude:e.row.content.venue.latitude,longitude:e.row.content.venue.longitude,title:e.row.content.artists[0].name,subtitle:e.row.content.venue.name,image:"images/guitar",animate:!0,customProperty:"Concert"}),a=Map.createView({mapType:Map.HYBRID,animate:!0,region:{latitude:e.row.content.venue.latitude,longitude:e.row.content.venue.longitude,latitudeDelta:.05,longitudeDelta:.05},regionFit:!1,height:300,width:400,bottom:300,userLocation:!0,annotations:[t]});return a.selectAnnotation(t),dataInfo.loadMaps(t,a),groupView.add(a),a}var Map=require("ti.map"),geolocation=require("geolocation");module.exports=createMap;