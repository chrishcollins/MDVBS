var latitude,longitude,geolocation=function(){Ti.Geolocation.purpose="Your location is needed to determine current location.",Ti.Geolocation.accuracy=Ti.Geolocation.ACCURACY_BEST,Titanium.API.info(Titanium.Network.networkType===Titanium.Network.NETWORK_NONE?" Network Not found ":" connection present "),Ti.Geolocation.getCurrentPosition(function(){dataInfo.loadBands(latitude,longitude),console.log(dataInfo),Ti.Geolocation.reverseGeocoder(latitude,longitude,function(e){var t=(JSON.parse(this.responseText),e.places.city),a=Ti.UI.createLabel({color:"#fff",text:t,top:"10%",height:Ti.UI.SIZE,textAlign:"center",font:{fontSize:"12",fontWeight:"bold"}});mainView.add(a)})})};exports.geolocation=geolocation;